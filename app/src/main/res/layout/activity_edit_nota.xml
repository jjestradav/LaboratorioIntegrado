<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.editNota.EditNotaActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="89dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="200dp"
        android:text="Cedula"
        app:layout_constraintEnd_toStartOf="@+id/cedulaText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cedulaText"
        android:layout_width="140dp"
        android:layout_height="31dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="200dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="85dp"
        android:layout_height="32dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="84dp"
        android:text="Nombre"
        app:layout_constraintEnd_toStartOf="@+id/nombreText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/nombreText"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:layout_marginTop="88dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cedulaText" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="83dp"
        android:layout_height="29dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="100dp"
        android:text="Nota"
        app:layout_constraintEnd_toStartOf="@+id/notaText"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/notaText"
        android:layout_width="148dp"
        android:layout_height="43dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="132dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombreText" />

    <ImageButton
        android:id="@+id/saveBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/save" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="195dp"
        android:layout_height="58dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="60dp"
        android:text="Registro de Nota"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>